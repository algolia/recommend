{
  "name": "@algolia/ui-components-horizontal-slider-react",
  "version": "1.0.0-experimental.2",
  "license": "MIT",
  "homepage": "https://github.com/algolia/ui-components",
  "repository": "algolia/ui-components",
  "author": {
    "name": "Algolia, Inc.",
    "url": "https://www.algolia.com"
  },
  "source": "src/index.ts",
  "types": "dist/esm/index.d.ts",
  "module": "dist/esm/index.js",
  "main": "dist/esm/index.js",
  "umd:main": "dist/umd/index.js",
  "unpkg": "dist/esm/index.js",
  "jsdelivr": "dist/esm/index.js",
  "sideEffects": false,
  "files": [
    "dist/"
  ],
  "scripts": {
    "build:clean": "rm -rf ./dist",
    "build:esm": "babel src --root-mode upward --extensions '.ts,.tsx' --out-dir dist/esm --ignore '**/*/__tests__/'",
    "build:types": "tsc -p ./tsconfig.declaration.json --outDir ./dist/esm",
    "build:umd": "cross-env BUILD=react rollup --config",
    "build": "yarn build:clean && yarn build:esm && yarn build:umd && yarn build:types",
    "prepare": "yarn build:esm && yarn build:types"
  },
  "dependencies": {
    "@algolia/ui-components-horizontal-slider-vdom": "1.0.0-experimental.2"
  },
  "peerDependencies": {
    "react": ">= 16.8.0 < 18",
    "react-dom": ">= 16.8.0 < 18"
  }
}
